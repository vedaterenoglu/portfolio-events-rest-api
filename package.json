{
  "name": "portfolio-events-api",
  "version": "0.0.1",
  "description": "Nest.js template with Clerk, Prisma, and TypeScript.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/vedaterenoglu/portfolio-events-rest-api.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "Prisma",
    "clerk",
    "typescript",
    "eslint",
    "prettier"
  ],
  "author": {
    "name": "Vedat Erenoglu",
    "email": "info@vedaterenoglu.com",
    "url": "https://vedaterenoglu.com",
    "linkedin": "https://www.linkedin.com/in/vedaterenoglu/"
  },
  "license": "Recruiter-Evaluation License",
  "homepage": "https://portfolio-events-rest-api.demo.vedaterenoglu.com",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json",
    "test:watch": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json --coverage",
    "test:unit": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json",
    "test:unit:coverage": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json --coverage",
    "test:unit:watch": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json --watch",
    "test:integration": "cross-env NODE_ENV=test jest --config ./test/jest-integration.json",
    "test:integration:coverage": "cross-env NODE_ENV=test jest --config ./test/jest-integration.json --coverage",
    "test:integration:watch": "cross-env NODE_ENV=test jest --config ./test/jest-integration.json --watch",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "test:e2e:coverage": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage",
    "test:e2e:watch": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --watch",
    "test:ci": "cross-env NODE_ENV=test npm run test:unit:coverage && npm run test:integration:coverage && npm run test:e2e:coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-unit.json --runInBand",
    "seed": "ts-node ./seed/seed-db.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.11.1",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "dompurify": "^3.2.6",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-security": "^3.0.1",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^26.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.12.11",
    "@testcontainers/postgresql": "^11.2.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-environment-node": "^30.0.4",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "testcontainers": "^11.2.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "zod-prisma-types": "^3.2.4"
  }
}
